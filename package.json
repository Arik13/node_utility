{
    "name": "node_utility",
    "version": "1.0.17",
    "description": "",
    "main": "lib/src/index.js",
    "types": "lib/src/index.d.ts",
    "scripts": {
        "start": "ts-node-dev --respawn --cls --exit-child --ignore-watch node_modules app.ts",
        "update": "npm update ts_utility",
        "test": "tsc --noEmit",
        "build": "rmdir lib && ttsc && ts-node src/fse/buildExports",
        "clean": "rmdir /s /q dist"
    },
    "author": "Arik Dicks",
    "license": "ISC",
    "dependencies": {
        "change-case": "^4.1.2",
        "module-alias": "^2.2.2",
        "ts_utility": "github:Arik13/ts_utility",
        "yaml": "^1.10.2"
    },
    "devDependencies": {
        "@types/node": "^14.0.1",
        "@types/uniqid": "^5.2.0",
        "nodemon": "^2.0.15",
        "ts-node": "^8.10.1",
        "ts-node-dev": "^1.1.8",
        "ttypescript": "^1.5.13",
        "typescript": "^4.3.2"
    },
    "_moduleAliases": {
        "@util": "./node_modules/ts_utility/dist/src/"
    },
    "files": [
        "/dist"
    ],
    "exports": {
        "./chart": "./lib/src/chart.js",
        "./fse/buildExports": "./lib/src/fse/buildExports.js",
        "./fse/DirMapInitializer": "./lib/src/fse/DirMapInitializer.js",
        "./fse": "./lib/src/fse/index.js",
        "./stringifier": "./lib/src/stringifier.js"
    },
    "typesVersions": {
        "*": {
            "chart": [
                "./lib/src/chart.d.ts"
            ],
            "fse/buildExports": [
                "./lib/src/fse/buildExports.d.ts"
            ],
            "fse/DirMapInitializer": [
                "./lib/src/fse/DirMapInitializer.d.ts"
            ],
            "fse": [
                "./lib/src/fse/index.d.ts"
            ],
            "stringifier": [
                "./lib/src/stringifier.d.ts"
            ]
        }
    }
}